@article{Kelly2010,
    title = "A person independent system for recognition of hand postures used in sign language",
    journal = "Pattern Recognition Letters",
    volume = "31",
    number = "11",
    pages = "1359 - 1368",
    year = "2010",
    issn = "0167-8655",
    doi = "https://doi.org/10.1016/j.patrec.2010.02.004",
    author = "Daniel Kelly and John McDonald and Charles Markham",
    keywords = "Feature representation, Moments, Size and shape, Object recognition, Tracking, Classifier design and evaluation",
    abstract = "We present a novel user independent framework for representing and recognizing hand postures used in sign language. We propose a novel hand posture feature, an eigenspace Size Function, which is robust to classifying hand postures independent of the person performing them. An analysis of the discriminatory properties of our proposed eigenspace Size Function shows a significant improvement in performance when compared to the original unmodified Size Function. We describe our support vector machine based recognition framework which uses a combination of our eigenspace Size Function and Hu moments features to classify different hand postures. Experiments, based on two different hand posture data sets, show that our method is robust at recognizing hand postures independent of the person performing them. Our method also performs well compared to other user independent hand posture recognition systems."
}


@article{Dahmani2014,
    title = "User-independent system for sign language finger spelling recognition",
    journal = "Journal of Visual Communication and Image Representation",
    volume = "25",
    number = "5",
    pages = "1240 - 1250",
    year = "2014",
    issn = "1047-3203",
    doi = "https://doi.org/10.1016/j.jvcir.2013.12.019",
    author = "Djamila Dahmani and Slimane Larabi",
    keywords = "Hand posture, Shape recognition, Hand segmentation, Tchebichef moments, Hu moments, Sign language, Recognition, Classification",
    abstract = "We propose in this paper a framework for recognizing the sign language alphabet. To separate hand images from complex backgrounds, we use skin colour and texture attributes with neural networks. The recognition process is based on the combination of three shape descriptors: Discrete orthogonal Tchebichef moments applied on both internal and external outlines hand, Hu moments and a set of geometric features derived from the convex hull that encloses the hand shape taking into account the hand orientation. The recognition is carried out using KNN and SVM classifiers. The proposed descriptors are combined in several sequential and parallel manners and applied on different datasets. The obtained results are compared to existing works."
}


@Article{Ferreira2018,
    author="Ferreira, Pedro M.
    and Cardoso, Jaime S.
    and Rebelo, Ana",
    title="On the role of multimodal learning in the recognition of sign language",
    journal="Multimedia Tools and Applications",
    year="2018",
    month="Sep",
    day="01",
    abstract="Sign Language Recognition (SLR) has become one of the most important research areas in the field of human computer interaction. SLR systems are meant to automatically translate sign language into text or speech, in order to reduce the communicational gap between deaf and hearing people. The aim of this paper is to exploit multimodal learning techniques for an accurate SLR, making use of data provided by Kinect and Leap Motion. In this regard, single-modality approaches as well as different multimodal methods, mainly based on convolutional neural networks, are proposed. Our main contribution is a novel multimodal end-to-end neural network that explicitly models private feature representations that are specific to each modality and shared feature representations that are similar between modalities. By imposing such regularization in the learning process, the underlying idea is to increase the discriminative ability of the learned features and, hence, improve the generalization capability of the model. Experimental results demonstrate that multimodal learning yields an overall improvement in the sign recognition performance. In particular, the novel neural network architecture outperforms the current state-of-the-art methods for the SLR task.",
    issn="1573-7721",
    doi="10.1007/s11042-018-6565-5",
}


